{
    "name": "vendi-advertising/vendi-asset-loader",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Haas",
            "email": "chris@vendiadvertising.com"
        }
    ],
    "require": {
        "webmozart/path-util": "^2.3",
        "webmozart/glob": "^4.1",
        "symfony/dotenv": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10.2",
        "sensiolabs/security-checker": "^5",
        "symfony/var-dumper": "^4",
        "phpunit/phpunit": "^7",
        "mikey179/vfsstream": "^1.6"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {"Vendi\\VendiAssetLoader\\": "src/"},
        "files": ["src/Functions.php"]
    }
}
